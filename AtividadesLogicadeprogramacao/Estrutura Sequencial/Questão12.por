programa {
  funcao inicio() {

    real salarioMinimo, salario, quantiadeMinimo

    escreva("Digite o seu salário: R$")
      leia(salario)
    escreva("Digite o valor do salario minimo hoje: R$")
      leia(salarioMinimo)

      quantiadeMinimo = salario / salarioMinimo

      escreva("\n Aquantidade de salarios minimos que você ganha é: ", quantiadeMinimo)

    
  }
}
